    "@types/jest": "^21.1.5",
    "@types/react": "^16.0.18",
    "@types/react-beautiful-dnd": "^2.3.0",
    "@types/react-dom": "^16.0.2",
    "@types/react-select": "^1.0.59",
    "@types/react-test-renderer": "^16.0.0",
    "classnames": "^2.2.5",
    "core-js": "^2.5.1",
    "enzyme": "^3.1.0",
    "http-server": "^0.10.0",
    "jest": "^21.3.0-beta.4",
